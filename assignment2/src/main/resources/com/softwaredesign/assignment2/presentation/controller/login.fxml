<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="200.0"
            prefWidth="400.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.softwaredesign.assignment2.presentation.controller.LoginController">
    <children>
        <Label layoutX="61.0" layoutY="93.0" text="Password:" />
        <Label layoutX="61.0" layoutY="40.0" text="Mail:" />
        <PasswordField fx:id="password" layoutX="167.0" layoutY="88.0" promptText="Enter password" />
        <TextField fx:id="mail" layoutX="167.0" layoutY="35.0" promptText="Enter mail" />
        <Button fx:id="loginButton" layoutX="167.0" layoutY="144.0" mnemonicParsing="false" onAction="#login" text="Login" />
    </children>
</AnchorPane>
